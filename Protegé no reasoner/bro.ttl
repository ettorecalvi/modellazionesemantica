                             owl:someValuesFrom :componenti_da_strada
                           ] ;
       rdfs:subClassOf :Bicicletta_da_Strada ;
       rdfs:comment "Una bicicletta da strada progettata per corse, con caratteristiche specifiche di montaggio." ;
       rdfs:label "Corsa" .


