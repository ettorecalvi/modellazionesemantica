#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#compete
:compete rdf:type owl:ObjectProperty ,
                  owl:TransitiveProperty ;
         rdfs:domain :Bicicletta ;
         rdfs:range :Bicicletta .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#effettuatoDa
:effettuatoDa rdf:type owl:ObjectProperty ;
              owl:inverseOf :possiedeOrdine ;
              rdfs:domain :Ordine ;
              rdfs:range :Cliente ;
              rdfs:comment "specifica che un Ordine è stato effettuato da un Cliente. E' l’inversa di possiedeOrdine" ;
              rdfs:label "effettuatoDa" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#guida
:guida rdf:type owl:ObjectProperty ;
       rdfs:domain :Cliente ;
       rdfs:range :Bicicletta ;
       owl:propertyChainAxiom ( :possiedeOrdine
                                :riguardaBicicletta
                              ) ;
       rdfs:comment "è una property chain che racchiude Cliente -> possiedeOrdine -> Ordine -> riguardaBicicletta -> Bicicletta, ed è da intendersi come “Un cliente possiede un ordine che riguarda una specifica bicicletta”." ;
       rdfs:label "guida" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#monta
:monta rdf:type owl:ObjectProperty ;
       owl:equivalentProperty <http://www.wikidata.org/entity/P527> ;
       rdfs:domain :Bicicletta ;
       rdfs:range :Componenti ;
       rdfs:comment "specifica che una data bicicletta è equipaggiata con una specifica componentistica" ;
       rdfs:label "monta" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#possiedeOrdine
:possiedeOrdine rdf:type owl:ObjectProperty ;
                rdfs:domain :Cliente ;
                rdfs:range :Ordine ;
                rdfs:comment "modella un ordine posseduto da un cliente" ;
                rdfs:label "possiedeOrdine" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#riguardaBicicletta
:riguardaBicicletta rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Ordine ;
                    rdfs:range :Bicicletta ;
                    rdfs:comment "indica che un ordine include una specifica bicicletta. E’ funzionale perchè un ordine può riguardare solo una specifica bicicletta alla volta" ;
                    rdfs:label "riguardaBicicletta" .


###  http://www.wikidata.org/entity/P527
<http://www.wikidata.org/entity/P527> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#disponibile
:disponibile rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Bicicletta ;
             rdfs:range xsd:boolean ;
             rdfs:comment "proprietà booleana che indica se una bicicletta è disponibile per la vendita o l'affitto." ;
             rdfs:label "disponibile" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#escursione
:escursione rdf:type owl:DatatypeProperty ;
            rdfs:domain :Bicicletta_da_Mountainbike ;
            rdfs:range xsd:integer ;
            rdfs:comment "proprietà che rappresenta l'escursione delle sospensioni di una bicicletta." ;
            rdfs:label "escursione" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#haDataFine
:haDataFine rdf:type owl:DatatypeProperty ;
            rdfs:domain :Ordine ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "fine del periodo di noleggio" ;
            rdfs:label "haDataFine" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#haDataInizio
:haDataInizio rdf:type owl:DatatypeProperty ;
              rdfs:domain :Ordine ;
              rdfs:range xsd:dateTime ;
              rdfs:comment "inizio del periodo di noleggio" ;
              rdfs:label "haDataInizio" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#prezzo
:prezzo rdf:type owl:DatatypeProperty ;
        rdfs:domain :Bicicletta ;
        rdfs:range xsd:int ;
        rdfs:comment "proprietà che rappresenta il prezzo di una bicicletta." ;
        rdfs:label "prezzo" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#taglia
:taglia rdf:type owl:DatatypeProperty ;
        rdfs:domain :Bicicletta ;
        rdfs:range xsd:string ;
        rdfs:comment "proprietà che rappresenta la taglia di una bicicletta." ;
        rdfs:label "taglia" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/vso/ns#Bicycle
<http://purl.org/vso/ns#Bicycle> rdf:type owl:Class ;
                                 owl:equivalentClass :Bicicletta .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta
:Bicicletta rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :monta ;
                                  owl:someValuesFrom :Componenti
                                ] ;
            rdfs:comment "Una bicicletta generica che monta una serie di componenti." ;
            rdfs:label "Bicicletta" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta_da_Mountainbike
:Bicicletta_da_Mountainbike rdf:type owl:Class ;
                            owl:equivalentClass <http://www.wikidata.org/entity/Q223705> ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :monta ;
                                                  owl:someValuesFrom :componenti_da_mountainbike
                                                ] ;
                            rdfs:subClassOf :Bicicletta ;
                            owl:disjointWith :Bicicletta_da_Strada ;
                            rdfs:comment "bicicletta progettata per l'utilizzo in percorsi off-road, con caratteristiche specifiche di montaggio di componenti adatti alla mountain bike." ;
                            rdfs:label "Bicicletta_da_Mountainbike" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta_da_Strada
:Bicicletta_da_Strada rdf:type owl:Class ;
                      owl:equivalentClass <http://www.wikidata.org/entity/Q6453072> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :monta ;
                                            owl:someValuesFrom :componenti_da_strada
                                          ] ;
                      rdfs:subClassOf :Bicicletta ;
                      rdfs:comment "bicicletta progettata per l'utilizzo su strade asfaltate, con caratteristiche specifiche di montaggio di componenti adatti alle biciclette da strada." ;
                      rdfs:label "Bicicletta_da_Strada" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#CX
:CX rdf:type owl:Class ;
    owl:equivalentClass [ rdf:type owl:Restriction ;
                          owl:onProperty :monta ;
                          owl:someValuesFrom :componenti_CX
                        ] ;
    rdfs:subClassOf :Bicicletta_da_Strada ;
    rdfs:comment "Una bicicletta da strada del tipo Cyclocross con caratteristiche specifiche di montaggio." ;
    rdfs:label "CX" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Cliente
:Cliente rdf:type owl:Class ;
         owl:equivalentClass <http://www.wikidata.org/entity/Q852835> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :possiedeOrdine ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Ordine
                             ] ;
         rdfs:comment "Una persona che possiede almeno un ordine di bicicletta." ;
         rdfs:label "Cliente" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Componenti
:Componenti rdf:type owl:Class ;
            owl:equivalentClass <http://www.wikidata.org/entity/Q30246160> ,
                                [ rdf:type owl:Class ;
                                  owl:oneOf ( :cerchi
                                              :copertone_CX
                                              :copertone_gravel
                                              :copertone_largo
                                              :copertone_strada
                                              :copertone_stretto
                                              :copertoni_DH
                                              :copertoni_Enduro
                                              :copertoni_Trail
                                              :copertoni_XC
                                              :forcella_rigida
                                              :frame
                                              :freni_160mm
                                              :freni_180mm
                                              :freni_200mm
                                              :freni_220mm
                                              :manubrio_a_goccia
                                              :manubrio_flat
                                              :reggisella_telescopico
                                              :sella_rigida
                                              :sospensioni_front
                                              :sospensioni_full
                                              <http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#copertoni_BMX/Dirt>
                                            )
                                ] ;
            rdfs:comment "nsieme di parti che compongono una bicicletta, come cerchi, copertoni, freni, etc." ;
            rdfs:label "Componenti" .


###  http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Corsa
:Corsa rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
