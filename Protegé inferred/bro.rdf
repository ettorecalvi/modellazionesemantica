    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#escursione -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#escursione">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta_da_Mountainbike"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
