    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#haDataInizio -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#haDataInizio">
