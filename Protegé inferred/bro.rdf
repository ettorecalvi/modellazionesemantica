    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#compete -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#compete">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#effettuatoDa -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#effettuatoDa">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#possiedeOrdine"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Ordine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Cliente"/>
        <rdfs:comment>specifica che un Ordine è stato effettuato da un Cliente. E&apos; l’inversa di possiedeOrdine</rdfs:comment>
        <rdfs:label>effettuatoDa</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#guida -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#guida">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Cliente"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#possiedeOrdine"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#riguardaBicicletta"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>è una property chain che racchiude Cliente -&gt; possiedeOrdine -&gt; Ordine -&gt; riguardaBicicletta -&gt; Bicicletta, ed è da intendersi come “Un cliente possiede un ordine che riguarda una specifica bicicletta”.</rdfs:comment>
        <rdfs:label>guida</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#monta -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#monta">
        <owl:equivalentProperty rdf:resource="http://www.wikidata.org/entity/P527"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Componenti"/>
        <rdfs:comment>specifica che una data bicicletta è equipaggiata con una specifica componentistica</rdfs:comment>
        <rdfs:label>monta</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#possiedeOrdine -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#possiedeOrdine">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Cliente"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Ordine"/>
        <rdfs:comment>modella un ordine posseduto da un cliente</rdfs:comment>
        <rdfs:label>possiedeOrdine</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#riguardaBicicletta -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#riguardaBicicletta">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Ordine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
        <rdfs:comment>indica che un ordine include una specifica bicicletta. E’ funzionale perchè un ordine può riguardare solo una specifica bicicletta alla volta</rdfs:comment>
        <rdfs:label>riguardaBicicletta</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.wikidata.org/entity/P527 -->

    <owl:ObjectProperty rdf:about="http://www.wikidata.org/entity/P527"/>
    


