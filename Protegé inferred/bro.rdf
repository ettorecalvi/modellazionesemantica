    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#prezzo -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#prezzo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>proprietà che rappresenta il prezzo di una bicicletta.</rdfs:comment>
        <rdfs:label>prezzo</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#taglia -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#taglia">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ettorecalvi/ontologies/2024/0/bicycle#Bicicletta"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>proprietà che rappresenta la taglia di una bicicletta.</rdfs:comment>
        <rdfs:label>taglia</rdfs:label>
    </owl:DatatypeProperty>
    


